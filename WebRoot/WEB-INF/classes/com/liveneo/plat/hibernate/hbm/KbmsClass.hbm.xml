<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liveneo.plat.hibernate.dao">
	<class
		name="KbmsClass"
		table="kbms_class"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Title"
			column="TITLE"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Pid"
			column="PID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Ord"
			column="ORD"
			type="string"
			not-null="false"
			length="32"
		/>


	</class>	
</hibernate-mapping>