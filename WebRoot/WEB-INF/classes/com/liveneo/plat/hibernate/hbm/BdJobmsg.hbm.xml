<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liveneo.plat.hibernate.dao">
	<class
		name="BdJobmsg"
		table="bd_jobmsg"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="Job_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="JobName"
			column="Job_name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="CronExpression"
			column="CronExpression"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="JobState"
			column="Job_state"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="CreateTime"
			column="Create_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="LastModifyTime"
			column="Last_modify_time"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="CreateOper"
			column="Create_oper"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="JobYear"
			column="Job_year"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="JobMonth"
			column="Job_month"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="JobDay"
			column="Job_day"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="JobWeek"
			column="Job_week"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="JobHour"
			column="Job_hour"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="JobMinute"
			column="Job_minute"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="JobSecond"
			column="Job_second"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="JobBeanname"
			column="Job_beanName"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Remark"
			column="remark"
			type="string"
			not-null="false"
			length="500"
		/>


	</class>	
</hibernate-mapping>