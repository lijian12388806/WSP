		<property
			name="Audittime"
			column="AUDITTIME"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Completeperson"
			column="COMPLETEPERSON"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Completetime"
			column="COMPLETETIME"
			type="timestamp"
			not-null="false"
			length="23"
