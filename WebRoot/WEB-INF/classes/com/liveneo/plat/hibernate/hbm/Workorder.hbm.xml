			name="Customername"
			column="CUSTOMERNAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Callerno"
			column="CALLERNO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Wostatus"
			column="WOSTATUS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Ordertype"
			column="ORDERTYPE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Projecttype"
			column="PROJECTTYPE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Workitem"
			column="WORKITEM"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Proname"
			column="PRONAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Procode"
			column="PROCODE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Ordersource"
			column="ORDERSOURCE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Generatemode"
			column="GENERATEMODE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Calltime"
			column="CALLTIME"
			type="timestamp"
			not-null="false"
			length="23"
		/>
		<property
			name="Dealtime"
