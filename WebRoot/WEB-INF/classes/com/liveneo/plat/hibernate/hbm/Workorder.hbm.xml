			name="Temp2"
			column="TEMP2"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Temp3"
			column="TEMP3"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Temp4"
			column="TEMP4"
			type="string"
			not-null="false"
			length="500"
