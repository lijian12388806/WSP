			name="Actusername"
			column="ACTUSERNAME"
			type="string"
			not-null="false"
			length="20"
		/>
		<many-to-one
			name="PkUserinfo"
			column="PK_USERINFO"
			class="Userinfo"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="PkCustomerinfo"
			column="PK_CUSTOMERINFO"
			class="Customerinfo"
			not-null="false"
		>
		</many-to-one>
	

		<set name="ORDERDEALINFOs" inverse="true">
			<key column="PK_WORKORDER"/>
			<one-to-many class="Orderdealinfo"/>
		</set>


	</class>	
</hibernate-mapping>