			name="Factory"
			column="FACTORY"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Agentno"
			column="AGENTNO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Ctipwd"
			column="CTIPWD"
