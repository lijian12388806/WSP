<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liveneo.plat.hibernate.dao">
	<class
		name="Ivrvoice"
		table="IVRVOICE"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="string"
			column="PK_IVRVOICE"
		>
			<generator class="native"/>
		</id>

		<property
			name="Leavewordnum"
			column="LEAVEWORDNUM"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="Serverip"
			column="SERVERIP"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Callerid"
			column="CALLERID"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Starttime"
			column="STARTTIME"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Endtime"
			column="ENDTIME"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Startdate"
			column="STARTDATE"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Enddate"
			column="ENDDATE"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Filename"
			column="FILENAME"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Isnotice"
			column="ISNOTICE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Ischeck"
			column="ISCHECK"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Dnis"
			column="DNIS"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="CreateDate"
			column="CREATE_DATE"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Processresult"
			column="PROCESSRESULT"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="Ucid"
			column="UCID"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Vduid"
			column="VDUID"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="N1"
			column="N1"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="N2"
			column="N2"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="N3"
			column="N3"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Fullstartdate"
			column="FULLSTARTDATE"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="Fullenddate"
			column="FULLENDDATE"
			type="string"
			not-null="false"
			length="24"
		/>


	</class>	
</hibernate-mapping>